package com.thzm.eye025.bean;

public class Department {
	
	int id;
	String name;
	String pictuer;
	String conten;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPictuer() {
		return pictuer;
	}
	public void setPictuer(String pictuer) {
		this.pictuer = pictuer;
	}
	public String getConten() {
		return conten;
	}
	public void setConten(String conten) {
		this.conten = conten;
	}
	
	
	public Department(int id, String name, String pictuer, String conten) {
		super();
		this.id = id;
		this.name = name;
		this.pictuer = pictuer;
		this.conten = conten;
	}
	
	
	public Department() {
		super();
	}
	
	@Override
	public String toString() {
		return "Department [id=" + id + ", name=" + name + ", pictuer=" + pictuer + ", conten=" + conten + "]";
	}
	
	
	
	

}
